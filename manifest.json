{
  "manifest_version": 3,
  "name": "ChromeTest",
  "version": "1.0",
  "description": "Collects basic system information from a Chromebook and saves it to a file.",
  "permissions": [
    "system.cpu",
    "system.memory",
    "system.storage",
    "fileSystem",
    "downloads"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}

